{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
        "files": ["./src/*", "./styles/*"]
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-cycle": 0,
        "no-unused-vars":"off",
        "import/extensions": 0,
        "no-undef": 0,
        "no-param-reassign": "off",
        "import/named": 0,
        "no-restricted-globals":"off",
        "radix":"off",
        "prefer-destructuring":"off",
        "no-return-assign":"off",
        "arrow-body-style": "off",
        "prefer-default-export": false
    }
}
